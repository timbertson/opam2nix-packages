  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1ripjp9biw4ahk4s0i66sd7bjwn7sbm9ajba1ajvxjkylq667v4a";
