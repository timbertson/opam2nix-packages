  installPhase = "${opam2nix}/bin/opam2nix invoke install";
  name = "ocplib-endian-0.8";
  opamEnv = builtins.toJSON 
  {
    deps = opamDeps;
    files = null;
    name = "ocplib-endian";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "0i7ar0kyqpwgw58nyxx5ymjzangd8zcprsdns36f16j5jbdgza9l";
    url = "https://github.com/OCamlPro/ocplib-endian/archive/0.8.tar.gz";
