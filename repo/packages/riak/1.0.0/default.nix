  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "1fqg4wfk465abs7ddv45riwfrv442dylp21x8n5pq9vhnr6izfal";
