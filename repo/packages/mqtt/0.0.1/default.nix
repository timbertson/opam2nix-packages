  {
    deps = opamDeps;
    files = null;
    name = "mqtt";
    ocaml-version = world.ocamlVersion;
    spec = ./opam;
  };
  passthru = 
  {
    opamSelection = opamSelection;
  };
  propagatedBuildInputs = inputs;
  src = fetchurl 
  {
    sha256 = "16b1shfw3rfjia72rfm7ap13k6qjk08jyvq8z042jb36cgaz2kan";
    url = "https://github.com/j0sh/ocaml-mqtt/archive/v0.0.1.tar.gz";
